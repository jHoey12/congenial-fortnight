{
    "externals": {
       "nugetv2": [
            {
                "name": "TaskModuleSqlUtility",
                "version": "0.1.5",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                      "source": "*.ps*",
                      "dest":"ps_modules/TaskModuleSqlUtility"
                    }
                ]
            },
            {
                "name": "VstsTaskSdk",
                "version": "0.8.2",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                        "source": [
                            "*.dll",
                            "*.ps1",
                            "*.psd1",
                            "*.psm1",
                            "lib.json",
                            "Strings"
                        ],
                        "dest": "ps_modules/VstsTaskSdk/",
                        "options": "-R"
                    }
                ]
            }
        ]
    },
    "cp": [
        {
            "source": [
                "*.sql"
            ]
        }
    ]
}